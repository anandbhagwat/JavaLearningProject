
public class Program_3_Fibonacci_Series {
	public static void main(String[] args) {
		
		int fibonacci = 0;
		int n1 = 0;
		int n2 = 1;
		int noffibonaccci = 5;
		
		while(noffibonaccci>0) {
			
			System.out.println(fibonacci);
			
			fibonacci = n1+n2;
			n1=n2;
			n2=fibonacci;
			noffibonaccci--;
		}
		
		}
	}
