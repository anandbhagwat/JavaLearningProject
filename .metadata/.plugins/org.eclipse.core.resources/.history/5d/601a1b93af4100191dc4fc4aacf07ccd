
public class Program_2_Factorial_Of_Number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		int n=2;
		int factorial=1;
		
		if (n<1)
		{
			factorial = 1;
		}
		
		while(n>=1)
		{
			factorial=factorial*n;
			n--;
		}
		
		System.out.println(factorial);
	}
}
